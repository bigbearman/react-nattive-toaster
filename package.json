{
  "name": "react-native-toaster-ui",
  "version": "1.0.1-alpha.3",
  "description": "A reusable toast notification module for React Native",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "keywords": [
    "react-native",
    "toast",
    "notification"
  ],
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.70.0",
    "lucide-react-native": ">=0.0.1 <0.476.0 || >=0.477.0",
    "typescript": "^4.9.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md}": "yarn prettier",
    "*.{js,jsx,ts,tsx}": "yarn lint"
  }
}